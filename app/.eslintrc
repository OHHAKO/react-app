{
  "parser": "babel-eslint",
  "rules": {
    "no-commonjs": 0,
    "strict": 0,
    "react/display-name": 0
  },
  "env": {
    "amd": true,
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
